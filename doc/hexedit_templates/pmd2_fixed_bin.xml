<?xml version="1.0"?>
<!DOCTYPE binary_file_format SYSTEM "BinaryFileFormat.dtd">
<binary_file_format name="FixedBinContents" type_name="" comment="" expr="">
	<define_struct type_name="FloorEntry" comment="" expr=""><data type="int" name="width" format="unsigned" len="2" display="dec"/><data type="int" name="height" format="unsigned" len="2" display="dec"/><data type="none" name="padding" len="2"/><for name="FOR_TilesEntries" count="" stop_test="CurrentTile &gt;= TotalTiles" type_name="" comment=""><struct name="TileEntry" type_name="" comment="" expr=""><data type="int" name="contentType" format="unsigned" len="2" display=""/><data type="int" name="length" format="unsigned" len="2" display=""/></struct></for><eval expr="TotalTiles = width * heigth; CurrentTile = 0;" display_error="false" display_result="false" comment=""/></define_struct><struct name="sir0_hdr" type_name="" comment="" expr="DataPtr: {this.ptrToC;hex}"><data type="string" name="SIR0" format="ascii" len="4" domain="this == &quot;SIR0&quot;" display=""/><data type="int" name="ptrToC" format="unsigned" len="4" display="hex"/><data type="int" name="ptrSir0" format="unsigned" len="4" display="hex"/><data type="none" name="paddding" len="4"/></struct><eval expr="TotalFloors =  ((sir0_hdr.ptrSir0 - sir0_hdr.ptrToC)  +  (sir0_hdr.ptrSir0 - sir0_hdr.ptrToC) % 16) / 4" display_error="false" display_result="true" comment=""/><jump offset="sir0_hdr.ptrToC" comment="" origin="start"><for name="FOR_TocEntries" count="TotalFloors" stop_test="EntryPointer.ptrEntry == 0xAAAAAAAA" type_name="" comment=""><struct name="EntryPointer" type_name="" comment="" expr=""><data type="int" name="ptrEntry" format="unsigned" len="4" display="hex"/><if test="ptrEntry != 0xAAAAAAAA" comment=""><jump offset="ptrEntry" comment="" origin="start"><data type="none" name="n" len="4"/></jump></if></struct></for></jump><data type="none" name="none"/></binary_file_format>
